{
  "functions": {
    "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"]
  },
  "hosting": {
    "site": "devdiv",
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/divination",
        "function": "divination"
      },
      {
        "source": "/makeimage",
        "function": "makeimage"
      },
      {
        "source": "/app",
        "function": "app"
      }
    ]
  }
}
